{
    "name": "guiziweb/bookstack-sdk-php",
    "description": "A PHP SDK for BookStack API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Camille Islasse",
            "email": "camille.islasse@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/http-client": "^6.4|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.88",
        "nunomaduro/phpinsights": "*",
        "phpstan/phpstan": "^1.0",
        "phpunit/php-code-coverage": "^11.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "symfony/phpunit-bridge": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Guiziweb\\BookStackClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guiziweb\\BookStackClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "preferred-install": {
            "phpstan/phpstan": "dist"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
